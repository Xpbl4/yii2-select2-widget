{
  "name": "xpbl4/yii2-select2-widget",
  "description": "The Select2 widget for Yii framework.",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","widget","select","select2"],
  "homepage": "https://github.com/Xpbl4/yii2-select2-widget",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Serge Mashkov",
      "email": "xpbl4@motorwars.ru",
      "homepage": "https://github.com/Xpbl4",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Xpbl4/yii2-select2-widget/issues?state=open",
    "source": "https://github.com/Xpbl4/yii2-select2-widget"
  },
  "repositories":[
    {
      "type": "git",
      "url": "https://github.com/Xpbl4/yii2-select2-widget.git"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "bower-asset/select2": "4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "xpbl4\\select2\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "asset-installer-paths": {
      "bower-asset-library": "vendor/bower"
    }
  }
}
